.list-item.event(data-venue=JSON.stringify(event.venue))
  .row
    .columns.twelve
      h2 Meet with us
    .columns.six.center.map
    .columns.six
      .columns.seven.mobile-two.center 
        p
          img(src="/images/meeting-team.jpg")
      .columns.five.mobile-two.details
        h4 Location 
        h5= event.venue.name
        a(href="https://maps.google.com/maps?q=#{event.venue.address_1}+#{event.venue.city},+#{event.venue.state}+#{event.venue.zip}")= event.venue.address_1
          br
          span= event.venue.city + ', ' + event.venue.state + ' ' + event.venue.zip
        h4 Time and Date
        a(href=event.event_url)
          p.time(data-time=event.time) fetching time...
